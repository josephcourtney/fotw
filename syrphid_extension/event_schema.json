    "userAgent": {
      "type": "string",
      "description": "User agent string of the browser."
    },
    "operatingSystem": {
      "type": "string",
      "description": "Operating system of the user's device."
    },
    "screenResolution": {
      "type": "string",
      "description": "Screen resolution of the user's device."
    },
    "extensionVersion": {
      "type": "string",
      "description": "Version of the browser extension."
    },
    "type": {
      "type": "string",
      "description": "Type of the event."
